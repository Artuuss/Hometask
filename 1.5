def factorial(n: int):
    number=2
    fact=1
    while number<=n:
        fact*=number
        number+=1
    return fact

def factorial_zeros(n: int):
    power=1
    zerosnumber=0
    while (5**power)<=n:
        zerosnumber+=n//(5**power)
        power+=1
    return(zerosnumber)

n=int(input())
print(factorial(n))
print(factorial_zeros(n))
